#include <stdio.h>
int main()
{
	int i = 0;
	int a = 0;
	int c = 0;
	int b = 0;
	int d = 0;
	for (i = 0; i < 21; i++);
	{
		while (scanf_s("%d", &a) != EOF)
	 {
			d++;
         if (a % 2 == 0)
		 {
           c++;
		 }
			
		if (a % 2 != 0)
		 {
	       b++;
		 }
		if (d == 20)
		{
			break;
		}
	 }
		
		
	}
	printf("%d\n", c);
	printf("%d", b);

	return 0;
}
